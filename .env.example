OPENCLAW_GATEWAY_TOKEN=your-gateway-token-here
OPENCLAW_GATEWAY_URL=https://your-openclaw-gateway.example.com

# Render sets PORT automatically. Keep this for local dev only.
PORT=3001

# Comma-separated list of allowed frontend origins.
ALLOWED_ORIGINS=https://expungement.legal,http://localhost:3001

# Optional Google Sheets integration
GOOGLE_SHEETS_SPREADSHEET_ID=your-spreadsheet-id
GOOGLE_CREDENTIALS_PATH=./google-credentials.json
# Alternative for Render (paste the full service account JSON as one env var)
GOOGLE_CREDENTIALS_JSON=
